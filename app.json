{
  "expo": {
    "name": "Visite o Congresso",
    "description": "Aplicativo desenvolvido durante o Desafio VisitApp do Congresso Nacional",
    "slug": "visite-o-congresso",
    "privacy": "public",
    "githubUrl": "https://github.com/danielfsousa/visite-o-congresso",
    "sdkVersion": "29.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.11",
    "orientation": "portrait",
    "primaryColor": "#0A142B",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0A142B"
    },
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#E6E6E6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "app.visiteocongresso",
      "buildNumber": "1.0.11",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Este app usa a câmera para escanear QRCodes referentes ao Conteúdo Interativo."
      }
    },
    "android": {
      "package": "app.visiteocongresso",
      "versionCode": 11,
      "adaptiveIcon": {
        "backgroundColor": "#0A142B"
      },
      "permissions": [
        "CAMERA"
      ]
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "danilo-lemes",
            "project": "visite-o-congresso",
            "authToken": "cadcb3d04ebd4e3eadb5672a32f0e79db740fdf4f6294d2d91355ccedb9259fb"
          }
        }
      ]
    }
  }
}